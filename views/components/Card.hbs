<article class='card mb-2'>
	<div class='card-body'>
		<p>{{item.origin}}</p>
		<p>{{item.shortURL}}</p>
		<a href='/eliminar/{{item._id}}' class='btn btn-danger'>Eliminar</a>
		<a href='/editar/{{item._id}}' class='btn btn-warning'>Editar</a>
		<button
			href=''
			data-short='{{item.shortURL}}'
			class='btn btn-info'
		>Copiar</button>
	</div>
</article>